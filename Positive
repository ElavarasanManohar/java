import java.util.*;
class Positive
{
    public static void main(String args[])
    {
        int n;
        Scanner s=new Scanner(System.in);
        n=s.nextInt();
        if(0<n)
        {
            System.out.print("Positive");
        }
        else if(0>n)
        {
            System.out.print("Negative");
        }
        else
        {
            System.out.print("Zero");
        }
    }
}
